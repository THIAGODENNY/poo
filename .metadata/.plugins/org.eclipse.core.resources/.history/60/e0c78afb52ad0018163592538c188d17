package semana01;

/*
 * Essa classe demonstra alguns tipos de dado
 */
public class Exemplo03 {
	
	//esse � o m�todo main
	public static void main(String[] args) {
		int a;
		float b;
		double c;
		char d;
		boolean e;
		long f;
		
		
		Integer g;
		Double h;
		Character i;
		String k;
		
		g = 10;
		
		System.out.println(g.toString() + "0");
		
		int idade = 3;
		float notaDaProva = 7.5f;
		double totalDaCompra = 200.99;
		
		
		System.out.println(notaDaProva);
		System.out.println(totalDaCompra);
		
		d = 'm';
		e = true;
		
		f = 100;
		
		k = "Aula de Java";
		
		System.out.println(k);
	}
}
