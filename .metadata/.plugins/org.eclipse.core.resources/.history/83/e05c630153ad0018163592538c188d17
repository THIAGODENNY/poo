package semana02;

public class Exemplo06 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		String s = "2";
		switch (s) {
		case "1": System.out.println("Um");
			break;
		case "2": System.out.println("Dois");
			break;
		case "9": System.out.println("Dois");
			break;
		default: System.out.println("Nove");
		}

	}

}
